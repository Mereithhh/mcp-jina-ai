{
  "name": "mcp-jina-ai",
  "version": "1.1.1",
  "description": "MCP server for Jina AI services",
  "type": "module",
  "author": "mereith",
  "bin": {
    "mcp-jina-ai": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm\" node index.ts",
    "client": "NODE_OPTIONS=\"--loader ts-node/esm\" node client-example.js",
    "inspector": "npx @modelcontextprotocol/inspector npm run dev",
    "prepare": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@types/express": "^5.0.2",
    "express": "^5.1.0",
    "mcp-evals": "^1.0.18",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
